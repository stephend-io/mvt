datasource db {
  provider     = "mysql"
  url          = env("DATABASE_URL")
  relationMode = "prisma"
}

generator client {
  provider = "prisma-client-js"
}

model Song {
  id     Int    @id @unique
  year   Int
  month  Int
  rank   Int
  title  String
  artist String
  links  Json
}
